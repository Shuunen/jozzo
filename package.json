{
  "$schema": "https://json.schemastore.org/package",
  "author": "Romain Racamier-Lafon <romain.racamier@gmail.com>",
  "bugs": "https://github.com/Shuunen/jozzo/issues",
  "dependencies": {
    "preact": "10.17",
    "shuutils": "7.3"
  },
  "contributors": [
    {
      "name": "Jordan Boyer"
    }
  ],
  "description": "A color sort puzzle game",
  "devDependencies": {
    "@preact/preset-vite": "2.5",
    "@vitest/coverage-v8": "0.34",
    "autoprefixer": "10.4",
    "eslint": "8.49",
    "eslint-config-hardcore": "41.2",
    "eslint-config-preact": "1.3",
    "eslint-plugin-tailwindcss": "3.13",
    "eslint-plugin-unicorn": "48.0",
    "postcss": "8.4",
    "repo-check": "1.33",
    "tailwindcss": "3.3",
    "typescript": "5.2",
    "vite": "4.4",
    "vite-plugin-svgr": "3.2",
    "vitest": "0.34"
  },
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/Shuunen/jozzo#readme",
  "keywords": [
    "color",
    "sort",
    "puzzle",
    "Jojo"
  ],
  "license": "GPL-3.0",
  "name": "jozzo",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Shuunen/jozzo.git"
  },
  "scripts": {
    "build": "vite build && echo build success",
    "check": "pnpm install && pnpm build && pnpm lint && repo-check && pnpm test && echo check success",
    "dev": "vite",
    "lint": "tsc --noEmit && echo tsc success && eslint --cache --fix --ext .js,.ts,.tsx . && echo lint success",
    "postversion": "git push && git push --tags && npm publish",
    "preversion": "pnpm check",
    "serve": "pnpm build && vite preview",
    "test": "vitest --coverage --run && echo test success",
    "test:bench": "vitest bench",
    "test:update": "vitest --run --update && echo test update success",
    "test:watch": "vitest --coverage"
  },
  "type": "module",
  "version": "0.0.0"
}